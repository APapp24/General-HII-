	journal = {Computational Intelligence \& Neuroscience},
	author = {Sadiq Fareed, Mian Muhammad and Raza, Ali and Zhao, Na and Tariq, Aqil and Younas, Faizan and Ahmed, Gulnaz and Ullah, Saleem and Jillani, Syeda Fizzah and Abbas, Irfan and Aslam, Muhammad},
