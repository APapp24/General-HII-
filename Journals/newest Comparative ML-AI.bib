	note = {Publisher: Pontificia Universidad Javeriana},
	keywords = {SUPPORT vector machines, BOGOTA (Colombia), classification and regression models, condición estructural, FORECASTING, gestión de activos del alcantarillado, máquinas de soporte vectorial (SVM), modelos de clasificación y regresión, PIPE, PROBABILITY density function, Sewer asset management, SEWER pipes, SEWERAGE, structural condition, support vector machines (SVM)},
	pages = {1--14},
